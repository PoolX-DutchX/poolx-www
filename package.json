{
  "name": "poolx-www",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "bignumber.js": "^9.0.0",
    "ethers": "^4.0.30",
    "formik": "^1.5.7",
    "formsy-react": "^1.1.4",
    "giveth-react-scripts": "1.1.4",
    "history": "^4.7.2",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "moment": "^2.29.4",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.15.0",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-input-token": "^1.1.2",
    "react-router-dom": "^5.0.1",
    "react-skylight": "0.5.1",
    "react-toastify": "^5.2.1",
    "react-toggle": "^4.0.2",
    "styled-components": "^4.3.1",
    "sweetalert": "^2.0",
    "web3": "^1.0.0-beta.33",
    "web3-react": "^5.0.4",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eslint": "eslint 'src/**/*.js*'",
    "eslint:fix": "npm run eslint --fix",
    "eject": "react-scripts eject",
    "prettify": "prettier --write 'src/**/*.js*'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PoolX-DutchX"
  },
  "author": {
    "name": "poolx",
    "email": "info@poolx.io",
    "url": "http://poolx.io"
  },
  "license": "GPL-3.0"
}
